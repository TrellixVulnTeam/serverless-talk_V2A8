{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "name": "etcd-cluster-client-service",
        "annotations": {
            "auth.istio.io/2379": "NONE"
        }
    },
    "spec": {
        "selector": {
            "etcd_cluster": "etcd-cluster",
            "app": "etcd"
        },
        "ports": [
            {
                "name": "http",
                "protocol": "TCP",
                "port": 2379,
                "targetPort": 2379,
                "nodePort": 32379
            }
        ],
        "type": "NodePort"
    }
}
